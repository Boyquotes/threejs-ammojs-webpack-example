{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",

    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["www/scripts/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
